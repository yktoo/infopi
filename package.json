    "main": "bootstrap.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dist": "ng build --prod --base-href ./",
        "electron": "ng build --base-href ./ && electron .",
        "package": "npm run dist && node package.js",
        "package:rpi": "npm run dist && node package.js rpi",
        "deploy": "npm run package:rpi && scp dist/installers/infopi_2.1.0_armhf.deb yrpi: && ssh yrpi sudo dpkg -i infopi_2.1.0_armhf.deb && ssh yrpi rm infopi_2.1.0_armhf.deb && ssh yrpi sudo reboot"
    },
